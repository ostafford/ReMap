{
  "expo": {
    "name": "ReMap",
    "slug": "remap-memory-atlas",
    "description": "Your Interactive Memory Atlas - Transform experiences into an interactive, personal atlas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "remap",
    "extra": {
      "foursquareApiKey": "37rxGxJd3/1rLD9h1iSY9aHDH1TD4HoRou5HYAH5Oo98="
    }
  }
}
